  "scripts": {
    "start": "node dist/server.bundle.js",
    "dev": "npm-run-all --parallel dev:server dev:client",
    "dev:server": "webpack --config tools/webpack.config.js --env.target=node",
    "dev:client": "webpack-dev-server --config tools/webpack.config.js --env.target=web",
    "build": "npm-run-all --parallel build:server build:client",
    "build:server": "webpack --config tools/webpack.config.js --env.target=node --env.production",
    "build:client": "webpack --config tools/webpack.config.js --env.target=web --env.production",
    "test": "mocha --opts tools/mocha.opts",
    "test:watch": "npm run test -- --watch"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "cors": "^2.8.1",
    "dotenv": "^4.0.0",
    "express": "^4.14.1",
